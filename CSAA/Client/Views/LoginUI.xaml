<Window x:Class="Client.Views.LoginUI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client.Views"
        mc:Ignorable="d"
        Title="LoginUI" Height="1080" Width="1920">
    <Grid x:Name="userNamelbl" MinWidth="1920" MinHeight="720">
        <Label Content="User Name" HorizontalAlignment="Left" Margin="28,78,0,0" VerticalAlignment="Top"/>
        <Label x:Name="passwordlbl" Content="Password" HorizontalAlignment="Left" Margin="28,138,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.205,0.364"/>
        <PasswordBox HorizontalAlignment="Left" Margin="136,146,0,0" VerticalAlignment="Top" Width="130"/>
        <TextBox HorizontalAlignment="Left" Height="18" Margin="136,87,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="130"/>
        <Image HorizontalAlignment="Left" Height="144" Margin="401,138,0,0" VerticalAlignment="Top" Width="138"/>
    </Grid>
</Window>
